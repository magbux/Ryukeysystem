        OptionMain.BackgroundTransparency = 0.5
        OptionMain.BorderSizePixel = 0
        OptionMain.Parent = OptionScreenGui
        local OptionCorner = Instance.new("UICorner", OptionMain)
        OptionCorner.CornerRadius = UDim.new(0, 6)
        local OptionTitleBar = Instance.new("TextLabel")
        OptionTitleBar.Size = UDim2.new(1, 0, 0, 22)
        OptionTitleBar.Position = UDim2.new(0, 0, 0, 0)
        OptionTitleBar.BackgroundColor3 = Color3.fromRGB(70,70,70)
        OptionTitleBar.BackgroundTransparency = 0.5
        OptionTitleBar.Text = "Select Hub"
        OptionTitleBar.TextColor3 = Color3.fromRGB(240,240,240)
        OptionTitleBar.Font = Enum.Font.GothamSemibold
        OptionTitleBar.TextSize = 14
        OptionTitleBar.Parent = OptionMain
        local OptionTitleCorner = Instance.new("UICorner", OptionTitleBar)
        OptionTitleCorner.CornerRadius = UDim.new(0, 4)
        local function CreateButton(text, position, callback)
            local button = Instance.new("TextButton")
            button.Size = UDim2.new(0.9, 0, 0, 24)
            button.Position = UDim2.new(0.05, 0, 0, position)
            button.BackgroundColor3 = Color3.fromRGB(100,100,100)
            button.BackgroundTransparency = 0.5
            button.TextColor3 = Color3.fromRGB(240,240,240)
            button.Text = text
            button.Font = Enum.Font.GothamSemibold
            button.TextSize = 12
            button.Parent = OptionMain
            local buttonCorner = Instance.new("UICorner", button)
            buttonCorner.CornerRadius = UDim.new(0, 4)
            button.MouseButton1Click:Connect(callback)
            return button
        end
        CreateButton("ryusai with auto load", 30, function()
            SendNotification("Hub", "Loading Ryusai with AutoLoad...")
            getgenv().AP_AutoLoad = game.PlaceId
            local qot = queue_on_teleport or (syn and syn.queue_on_teleport) or (fluxus and fluxus.queue_on_teleport)
            if qot then
                pcall(function()
                    qot('loadstring(game:HttpGet("https://raw.githubusercontent.com/magbux/ryurakio/refs/heads/main/ryusai"))()')
                end)
            end
            loadstring(game:HttpGet("https://raw.githubusercontent.com/magbux/ryurakio/refs/heads/main/ryusai", true))()
            ScreenGui:Destroy()
            OptionScreenGui:Destroy()
        end)
        CreateButton("ryusai without auto load", 60, function()
