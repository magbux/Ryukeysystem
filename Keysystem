            getgenv().AP_AutoLoad = nil
            loadstring(game:HttpGet("https://raw.githubusercontent.com/magbux/ryurakio-without-load/refs/heads/main/no%20auto%20load%20yippe", true))()
            ScreenGui:Destroy()
            OptionScreenGui:Destroy()
        end)
        CreateButton("ryusai command hub", 90, function()
            SendNotification("Hub", "Loading Command Hub...")
            getgenv().AP_AutoLoad = nil
            loadstring(game:HttpGet("https://raw.githubusercontent.com/magbux/ryu-command-hub/refs/heads/main/ryu%20commandment", true))()
            ScreenGui:Destroy()
            OptionScreenGui:Destroy()
        end)
    end
    -- Button Functions
    Submit.MouseButton1Click:Connect(function()
