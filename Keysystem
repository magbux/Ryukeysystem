local function CreateOptionGUI()
    local OptionScreenGui = Instance.new("ScreenGui")
    OptionScreenGui.Name = "RyusaiOptionHub"
    OptionScreenGui.ResetOnSpawn = false
    OptionScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

    local OptionMain = Instance.new("Frame")
    OptionMain.Size = UDim2.new(0, 0, 0, 0)
    OptionMain.Position = UDim2.new(0.5, -100, 0.5, -90)
    OptionMain.BackgroundColor3 = Colors.Primary
    OptionMain.BackgroundTransparency = 0.5
    OptionMain.BorderSizePixel = 0
    OptionMain.Parent = OptionScreenGui
    AddCorner(OptionMain, 12)
    AddStroke(OptionMain, Colors.Accent, 0.5, 1.5)

    local OptionTitleBar = Instance.new("TextLabel")
    OptionTitleBar.Size = UDim2.new(1, 0, 0, 35)
    OptionTitleBar.BackgroundColor3 = Colors.Secondary
    OptionTitleBar.BackgroundTransparency = 0.5
    OptionTitleBar.Text = "--{Select Hub}--"
    OptionTitleBar.TextColor3 = Colors.Text
    OptionTitleBar.Font = Enum.Font.GothamBlack
    OptionTitleBar.TextSize = 14
    OptionTitleBar.Parent = OptionMain
    AddCorner(OptionTitleBar, 12)
    MakeDraggable(OptionTitleBar, OptionMain)

    local OptionCloseButton = Instance.new("TextButton")
    OptionCloseButton.Size = UDim2.new(0, 24, 0, 24)
    OptionCloseButton.Position = UDim2.new(1, -30, 0, 5)
    OptionCloseButton.BackgroundColor3 = Colors.Error
    OptionCloseButton.BackgroundTransparency = 0.4
    OptionCloseButton.Text = "X"
    OptionCloseButton.TextColor3 = Colors.Text
    OptionCloseButton.Font = Enum.Font.GothamBlack
    OptionCloseButton.TextSize = 14
    OptionCloseButton.Parent = OptionTitleBar
    AddCorner(OptionCloseButton, 6)
    OptionCloseButton.MouseButton1Click:Connect(function()
        TweenService:Create(OptionMain, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {Size = UDim2.new(0, 200, 0, 0)}):Play()
        wait(0.3)
        OptionScreenGui:Destroy()
    end)
    AddHoverEffect(OptionCloseButton, Colors.Error, Color3.fromRGB(220, 120, 120))

    local ScrollingFrame = Instance.new("ScrollingFrame")
    ScrollingFrame.Size = UDim2.new(1, 0, 1, -35)
    ScrollingFrame.Position = UDim2.new(0, 0, 0, 35)
    ScrollingFrame.BackgroundTransparency = 1
    ScrollingFrame.ScrollBarThickness = 3
    ScrollingFrame.ScrollBarImageColor3 = Colors.Accent
    ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    ScrollingFrame.Parent = OptionMain

    local OptionsList = Instance.new("UIListLayout")
    OptionsList.SortOrder = Enum.SortOrder.LayoutOrder
    OptionsList.Padding = UDim.new(0, 8)
    OptionsList.HorizontalAlignment = Enum.HorizontalAlignment.Center
    OptionsList.Parent = ScrollingFrame

    local Padding = Instance.new("UIPadding")
    Padding.PaddingTop = UDim.new(0, 8)
    Padding.PaddingBottom = UDim.new(0, 8)
    Padding.PaddingLeft = UDim.new(0, 8)
    Padding.PaddingRight = UDim.new(0, 8)
    Padding.Parent = ScrollingFrame

    local function CreateButton(text, url)
        local button = Instance.new("TextButton")
        button.Size = UDim2.new(0.9, 0, 0, 28)
        button.BackgroundColor3 = Colors.Secondary
        button.BackgroundTransparency = 0.5
        button.TextColor3 = Colors.Text
        button.Text = text
        button.Font = Enum.Font.GothamBlack
        button.TextSize = 12
        button.Parent = ScrollingFrame
        AddCorner(button, 8)
        AddStroke(button, Colors.Accent, 0.5, 1)
        AddHoverEffect(button, Colors.Secondary, Color3.fromRGB(110, 110, 110))

        button.MouseButton1Click:Connect(function()
            SendNotification("Loading", "Initializing " .. text .. "...", 2)
            getgenv().AP_AutoLoad = nil
            local script = HttpGet(url)
            if script then
                pcall(loadstring(script))
            else
                SendNotification("Error", "Failed to load hub.", 3)
            end
            TweenService:Create(OptionMain, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {Size = UDim2.new(0, 200, 0, 0)}):Play()
            wait(0.3)
            OptionScreenGui:Destroy()
        end)
        return button
    end

    CreateButton("New Project", "https://raw.githubusercontent.com/magbux/ryurakio20xbMnIk666/refs/heads/main/yoyoryusaihub")
    CreateButton("Fluent Version", "https://raw.githubusercontent.com/magbux/ryurakio-without-load/refs/heads/main/no%20auto%20load%20yippe")
    CreateButton("Command Hub", "https://raw.githubusercontent.com/magbux/ryu-command-hub/refs/heads/main/ryu%20commandment")
    CreateButton("Shader Hub", "https://raw.githubusercontent.com/magbux/shader-hub-lala/refs/heads/main/shader%20hub")
    CreateButton("Unknown Hub", "https://raw.githubusercontent.com/magbux/Unknown-hub/refs/heads/main/Unknown%20hub")

    OptionMain.Visible = true
    TweenService:Create(OptionMain, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Size = UDim2.new(0, 200, 0, 210)}):Play()
end

Submit.MouseButton1Click:Connect(function()
    local key = KeyBox.Text:match("^%s*(.-)%s*$")

    if ValidateKey(key) then
        SendNotification("Success", "Correct key!", 2)
        StatusLabel.Text = ""
        TweenService:Create(Main, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {Size = UDim2.new(0, 200, 0, 0)}):Play()
        wait(0.3)
        ScreenGui:Destroy()
        CreateOptionGUI()
    else
        SendNotification("Error", "Invalid key. Try again.", 2)
        StatusLabel.Text = "Invalid key"
        StatusLabel.TextColor3 = Colors.Error

        local originalPos = Main.Position
        for i = 1, 2 do
            TweenService:Create(Main, TweenInfo.new(0.04), {Position = originalPos + UDim2.new(0, 6, 0, 0)}):Play()
            wait(0.04)
            TweenService:Create(Main, TweenInfo.new(0.04), {Position = originalPos - UDim2.new(0, 6, 0, 0)}):Play()
            wait(0.04)
        end
        TweenService:Create(Main, TweenInfo.new(0.04), {Position = originalPos}):Play()
        wait(2)
        StatusLabel.Text = ""
    end
end)

CopyButton.MouseButton1Click:Connect(function()
    local randomKey = VALID_KEYS[math.random(1, #VALID_KEYS)]
    pcall(function() setclipboard(randomKey) end)
    SendNotification("Copied", "Random key copied to clipboard", 2)
    StatusLabel.Text = "Key copied!"
    StatusLabel.TextColor3 = Colors.Success
    wait(2)
    StatusLabel.Text = ""
end)
