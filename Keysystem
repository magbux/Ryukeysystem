    local function CreateOptionGUI()
        local OptionScreenGui = Instance.new("ScreenGui")
        OptionScreenGui.Name = "RyuOptionHub"
        OptionScreenGui.ResetOnSpawn = false
        OptionScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
